<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminActivity">

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:onClick="RegisterNewClient"
        android:text="@string/register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextpassword_admin"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edittextfrom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextnumberplate"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewusername_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:text="@string/username"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextfrom"
        app:layout_constraintEnd_toStartOf="@+id/edittextfrom"
        app:layout_constraintTop_toTopOf="@+id/edittextfrom"
        app:layout_constraintVertical_bias="0.73" />

    <EditText
        android:id="@+id/edittextpassword_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.395"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextemail_admin"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewpassword_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="57dp"
        android:text="@string/password"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextpassword_admin"
        app:layout_constraintEnd_toStartOf="@+id/edittextpassword_admin"
        app:layout_constraintTop_toTopOf="@+id/edittextpassword_admin" />

    <EditText
        android:id="@+id/edittextemail_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextprice"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewemail_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="@string/email_address"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextemail_admin"
        app:layout_constraintEnd_toStartOf="@+id/edittextemail_admin"
        app:layout_constraintTop_toTopOf="@+id/edittextemail_admin" />

    <EditText
        android:id="@+id/edittextnumberplate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="30dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewname_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="85dp"
        android:text="@string/name"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextnumberplate"
        app:layout_constraintEnd_toStartOf="@+id/edittextnumberplate"
        app:layout_constraintTop_toTopOf="@+id/edittextnumberplate"
        app:layout_constraintVertical_bias="0.73" />

    <EditText
        android:id="@+id/edittextto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextfrom"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewcnic_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="85dp"
        android:text="@string/cnic"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextto"
        app:layout_constraintEnd_toStartOf="@+id/edittextto"
        app:layout_constraintTop_toTopOf="@+id/edittextto"
        app:layout_constraintVertical_bias="0.653" />

    <EditText
        android:id="@+id/edittextprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edittextto"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textviewphone_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="65dp"
        android:text="@string/phone_no"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/edittextprice"
        app:layout_constraintEnd_toStartOf="@+id/edittextprice"
        app:layout_constraintTop_toTopOf="@+id/edittextprice" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/register_new_client"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>